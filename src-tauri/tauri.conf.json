{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
      "withGlobalTauri": true
  },
  "package": {
    "productName": "QIDI RFID Tag Guide",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.qidi.rfidguide",
      "icon": [
        "icons/icon_32x32.png",
        "icons/icon_128x128.png",
        "icons/icon_256x256.png",
        "icons/icon_512x512.png"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "RFID Tag Guide for QIDI BOX",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ]
  }
}

